{
  "name": "ng-dynamic-json-from-docs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "watch:lib": "ng build ng-dynamic-json-form --watch",
    "build:lib": "ng build ng-dynamic-json-form",
    "build:docs": "node scripts/build-docs-site.js",
    "build:config-schema": "node ./schema/config-schema-code-builder.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev:ssr": "ng run docs:serve-ssr --port 4201",
    "serve:ssr": "node dist/docs/server/main.js",
    "build:ssr": "ng build && ng run docs:server",
    "prerender:get-routes": "node scripts/get-prerender-routes.js",
    "prerender:dev": "npm run prerender:get-routes && ng run docs:prerender-dev --no-guess-routes --routes-file routes.txt",
    "prerender": "npm run prerender:get-routes && ng run docs:prerender --no-guess-routes --routes-file routes.txt"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/elements": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/material": "^16.2.14",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/platform-server": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@angular/ssr": "^17.3.17",
    "ajv": "^8.13.0",
    "angular-imask": "^7.6.1",
    "angular-split": "^15.0.0",
    "bootstrap-icons": "^1.11.2",
    "express": "^4.15.2",
    "highlight.js": "^11.9.0",
    "imask": "^7.6.1",
    "marked": "^12.0.2",
    "marked-gfm-heading-id": "^3.1.3",
    "marked-highlight": "^2.1.1",
    "primeng": "^16.9.1",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.2",
    "vanilla-jsoneditor": "^0.23.2",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular/cli": "^17.3.17",
    "@angular/compiler-cli": "^17.3.12",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^14.18.63",
    "autoprefixer": "^10.4.16",
    "browser-sync": "^3.0.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.2.2",
    "ng-packagr": "^17.3.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "ts-morph": "^26.0.0",
    "typescript": "~5.4.5",
    "wait-on": "^8.0.3"
  }
